<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
   
   
   
   
   
  <mapper namespace="mapper.member">
   <!-- 
  	<resultMap type="memberVO" id="memberResult">
  		<result property="VO컬럼명" column="DB컬럼명" />
  		<result property="VO컬럼명" column="DB컬럼명" />
  		<result property="VO컬럼명" column="DB컬럼명" />
  		<result property="VO컬럼명" column="DB컬럼명" />
  		<result property="VO컬럼명" column="DB컬럼명" />
  	</resultMap>
   -->
  
  	<select id="selectname" resultMap="String" >
  		<![CDATA[ 
  			select name from member
  			where id = '1'
  		]]>
  	</select>
  	
  	<insert id="insertMember" parameterType="MemberVO">
  		<![CDATA[
  			insert into member(name, id, password, email, 
  				phone, location, profile_img, status, joinDate)
  			values(#{name}, #{id}, #{password}, #{email}, 
  				#{phone}, #{location}, #{profile_img}, #{status}, #{joinDate})
  		]]>
  	</insert>
  	
  	<delete id="deleteMember" parameterType="String">
  		<![CDATA[
  			delete from member
  			where
  			id=#{id}
  		]]>
  	</delete>
  	
  	
  </mapper>